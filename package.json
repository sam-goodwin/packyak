{
	"name": "packyak",
	"version": "0.0.0",
	"type": "module",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"bin": {
		"packyak": "./bin/packyak.mjs"
	},
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.js"
		},
		"./aws-cdk": {
			"import": "./lib/aws-cdk/index.js",
			"require": "./lib/aws-cdk/index.js",
			"types": "./lib/aws-cdk/index.d.ts"
		}
	},
	"files": [
		"bin",
		"lib"
	],
	"scripts": {
		"watch": "tsc -b -w",
		"codegen": "poetry run python ./scripts/codegen.py",
		"typecheck": "poetry run pyright",
		"build": "pnpm typecheck && pnpm codegen && tsc -b",
		"synth": "poetry run python ./examples/app.py",
		"synth:website": "cdk synth --app 'tsx ./packyak.config.ts'",
		"publish:pypi": "poetry build && poetry publish"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@aws-cdk/aws-lambda-python-alpha": "2.118.0-alpha.0",
		"@biomejs/biome": "^1.5.3",
		"@tsconfig/node20": "^20.1.2",
		"@types/node": "^20.10.8",
		"aws-cdk": "2.118.0",
		"aws-cdk-lib": "2.118.0",
		"bun": "^1.0.22",
		"constructs": "10.3.0",
		"sst": "^2.39.5",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"@aws-cdk/aws-lambda-python-alpha": "2.118.0-alpha.0",
		"aws-cdk-lib": "2.118.0",
		"constructs": "^10.3.0"
	},
	"peerDependenciesMeta": {
		"constructs": {
			"optional": true
		},
		"aws-cdk-lib": {
			"optional": true
		},
		"@aws-cdk/aws-lambda-python-alpha": {
			"optional": true
		}
	},
	"dependencies": {
		"@aws-cdk/aws-glue-alpha": "2.126.0-alpha.0",
		"@aws-cdk/aws-sagemaker-alpha": "2.126.0-alpha.0"
	}
}
