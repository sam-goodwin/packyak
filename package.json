{
	"name": "@packyak/packyak",
	"version": "0.1.3",
	"type": "module",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.js"
		}
	},
	"scripts": {
		"watch": "tsc -b -w",
		"codegen": "poetry run python ./scripts/codegen.py",
		"typecheck": "poetry run pyright",
		"build": "pnpm typecheck && tsc -b",
		"synth": "poetry run python ./examples/app.py",
		"synth:website": "cdk synth --app 'tsx ./packyak.config.ts'",
		"publish:pypi": "poetry build && poetry publish"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "^1.5.3",
		"@tsconfig/node20": "^20.1.2",
		"@types/node": "^20.10.8",
		"aws-cdk-lib": "^2.127.0",
		"bun": "^1.0.22",
		"constructs": "10.3.0",
		"sst": "^2.39.5",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	}
}
