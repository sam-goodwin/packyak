{
  "files.exclude": {
    "**/__pycache__": true,
    ".pycache": true,
    ".ruff_cache": true
  },
  "editor.formatOnSave": true,
  "prettier.resolveGlobalModules": false,
  "editor.indentSize": 2,
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.indentSize": 2,
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.indentSize": 2,
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.indentSize": 4
  },
  "python.languageServer": "Default",
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.autoImportCompletions": true,
  "python.defaultInterpreterPath": ".venv/bin/python",
  "python.pyright.enabled": true,
  "python.pyright.autoImportCompletions": true,
  "python.pyright.typeCheckingMode": "strict",
  "cSpell.words": [
    "aioboto",
    "aiobotocore",
    "aiofiles",
    "boto",
    "botocore",
    "certificatemanager",
    "cgroups",
    "dagster",
    "dedupe",
    "Fargate",
    "Gids",
    "HDFS",
    "hudi",
    "JDBC",
    "JSII",
    "Lakehouse",
    "Lakehouses",
    "Milvus",
    "MWAA",
    "Nessie",
    "OLAP",
    "OLTP",
    "packyak",
    "projectnessie",
    "pydantic",
    "pyspark",
    "Qdrant",
    "quarkus",
    "selectbox",
    "streamlit",
    "thriftserver",
    "transactionally",
    "Unversioned",
    "VCPU",
    "virtualenv",
    "Weaviate",
    "xlarge"
  ],
}
